<div class="container">
<div class="full-width-back"></div>
  <div class="full-width">
<% @images.each do|f| %>
	
	<div class="col-sm-6 col-md-4">
    <div class="thumbnail tilt-tag" id="tag_photo_back">
  <p class="topic"><%= f.title %></p>
	<% if /missing.png/.match(f.snaps_url) %>
	<%else%>
	<br><%= link_to image_tag(f.snaps_url(:small)), f.snaps_url %><br>
	<center>
<p class="caption">

<%= link_to "Go To Album", user_album_path(@user.id,f.album.id), class: "btn btn-primary pull-bottom", role: "button" %>
</p>
</center>
</div>
</div>


<%end%>
<%end%>
</div>
	
	</div>