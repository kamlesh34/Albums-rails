<div class="container">
<div class="jumbotron">
  <h1>Tags</h1>
 </div>
   


<% @tags.each do |f| %>
<div class="col-sm-6 col-md-3">
<div class="thumbnail">
<% if f.photos.present? %>
<td><%= link_to image_tag(f.photos.first.image.url), '#', class: "thumbnail" %> </td>


<% else %>
<%#debugger%>
<td><img src="http://i.imgur.com/vGsqNpo.jpg"), class="thumbnail"></img> </td>
<% end %>



 <div class="caption">
<h3 ><a href="<%= tag_path(f.id) %>" class="btn btn-success btn-lg"><%= f.tname %>    <span class="badge"><%= f.photos.count %></span></a><br></h3>
</div>
</div>
</div>
<%end%>
</div>